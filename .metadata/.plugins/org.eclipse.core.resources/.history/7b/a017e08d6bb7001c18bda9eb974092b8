package com.april7;
class A  extends Thread{
    public void run() {
    	for(int i=0;i<2000;i++) {
    	System.out.println("thread--"+i);
    	}
    }
}
public class q6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
         Thread a=new Thread();
         A a1=new A();
         a1.run();
        for(int i=0;i<2000;i++) {
        	System.out.println("main--"+i);
        }
	}

}
