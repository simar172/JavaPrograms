package com.april7;

public class q1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        Thread t1=new Thread() {
        	public void run() {
        		for(int i=0;i<2000;i++) {	
        			System.out.println("thread-"+i);
        		}
        	
        	}
        	
        };
       t1.start();
        for(int i=0;i<2000;i++) {
    		System.out.println("main-"+i);
    	}
	}

}
