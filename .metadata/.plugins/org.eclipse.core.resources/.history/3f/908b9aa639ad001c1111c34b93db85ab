  package recur;

public class placingtiles {
     static int a(int n,int m) {
    	 if(n==m) {
    		 return 2;
    	 }
    	 if(n<m) {
    		 return 1;
    	 }
    	 int hori=a(n-1,m);
    	 int vert=0;
    	 return hori+vert;
     }
	public static void main(String[] args) {
		// TODO Auto-generated method stub
        System.out.println(a(4,2));
	}

}
